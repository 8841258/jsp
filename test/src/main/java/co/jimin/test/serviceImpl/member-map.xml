<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.jimin.test.service.MemberMapper">
  <select id="memberSelect" resultType="MemberVO">
		select * from member
  </select>
  
  <insert id="memberInsert" parameterType="MemberVO">
  		insert into member values(#{memberId}, #{memberName}, #{memberPhone}, #{memberAddr}, #{memberBirth})
  </insert>
  
  <update id="memberUpdate" parameterType="MemberVO">
  		update member
  		set member_name = #{memberName},
  		    member_phone = #{memberPhone},
  		    member_addr = #{memberAddr},
  		    member_birth =  #{memberBirth}
  		where member_id = #{memberId}
  </update>
  
  <delete id="memberDelete" parameterType="MemberVO">
  		delete member where member_id = #{memberId}
  </delete>
  
</mapper>